{
	"name": "@abacatepay/better-auth",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AbacatePay/ecosystem.git"
	},
	"main": "./dist/index.js",
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"typescript": "^5.0.0"
	},
	"author": {
		"name": "AbacatePay",
		"email": "opensource@abacatepay.com",
		"url": "https://abacatepay.com"
	},
