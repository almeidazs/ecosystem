{
	"version": "2.0.2",
	"name": "@abacatepay/types",
	"description": "Abacate Pay API typings that are always up to date.",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"license": "Apache-2.0",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./v1": {
			"import": "./dist/v1/index.js",
			"types": "./dist/v1/index.d.ts"
		},
		"./v1/*": {
			"import": "./dist/v1/*.js",
			"types": "./dist/v1/*.d.ts"
		},
		"./v2": {
			"import": "./dist/v2/index.js",
			"types": "./dist/v2/index.d.ts"
		},
		"./v2/*": {
			"import": "./dist/v2/*.js",
			"types": "./dist/v2/*.d.ts"
		},
		"./v*": null
	},
	"typesVersions": {
		"*": {
			"v1": [
				"dist/v1/index.d.ts"
			],
			"v2": [
				"dist/v2/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "bun run gen:version && tsc",
		"prepublishOnly": "bun run build",
		"gen:version": "bun run scripts/version.ts",
		"test": "bun test",
		"fmt": "bunx biome check --write package.json types && clear"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"typescript": "^5.9.3"
	},
	"author": {
		"name": "AbacatePay",
		"email": "opensource@abacatepay.com",
		"url": "https://abacatepay.com"
	},
	"maintainers": [
		{
			"name": "Almeida",
			"url": "https://github.com/almeidazs"
		}
	],
	"keywords": [
		"api",
		"abacatepay",
		"types",
		"typescript"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AbacatePay/ecosystem.git"
	}
}
